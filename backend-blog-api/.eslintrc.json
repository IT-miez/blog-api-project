{
    "env": {
        "node": true
    },
    "extends": ["eslint:recommended", "airbnb"],
    "ignorePatterns": ["templateController.js"],
    "rules": {
        "consistent-return": "off",
        "no-underscore-dangle": "off",
        "no-unused-vars": "off",
        "no-shadow": "off",
        "max-len": "off"
    }
}
